#define MAX(a,b) (a)>(b)?(a):(b)
int maxSubArray(int* nums, int numsSize){
    int max = nums[0];
    int* dp = (int*)malloc(numsSize*sizeof(int));
    dp[0] = nums[0];
    for(int i=1;i<numsSize;i++){
        dp[i] = MAX(dp[i-1]+nums[i],nums[i]);
        max = MAX(max, dp[i]);
    }
    return max;
}
