int removeDuplicates(int* nums, int numsSize){
    if(numsSize<=2) return numsSize;
    int index = 0;
    int cnt = 0;
    for(int i=1;i<numsSize;i++){
        if(nums[i]!=nums[index]){
            index++;
            nums[index] = nums[i];
            cnt = 0;
        }
        else if(nums[i]==nums[index] && cnt<1){
            index++;
            nums[index] = nums[i];
            cnt++;
        }
        else{
            cnt++;
        }
    }
    return index+1;
}
